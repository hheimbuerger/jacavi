<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
 	
 	<bean id="raceTimerIntervalValueBean" class="java.lang.Integer">
  		<constructor-arg value="{$raceTimerInterval}"/>
 	</bean>
 	
 	<!-- This Bean is to configure all non windows technologies see de.jacavi.hal.CarreraLibraryType -->
 	<bean id="noWindowsHALTechnologiesBean" class="org.springframework.beans.factory.config.ListFactoryBean">
    	<property name="sourceList">
    	  <list value-type="java.lang.String">
    	    <value>lib42</value>
    	  </list> 
    	</property>
  	</bean>
 	
 	
 	
 	
 	
</beans>